{
  "name": "cookie-though",
  "version": "1.7.0",
  "license": "MIT",
  "homepage": "https://cookiethough.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/inthepocket/cookie-though"
  },
  "keywords": [
    "cookies",
    "cookie",
    "cookie preferences",
    "browser",
    "universal"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build:site": "tsc --noEmit && parcel build src/index.html --no-source-maps --global CookieThough --out-dir public --no-content-hash && ./scripts/replace_css.sh staging",
    "build:lib": "parcel build src/lib.ts --no-source-maps --global CookieThough --out-dir dist --experimental-scope-hoisting && ./scripts/replace_css.sh",
    "build:docs": "astro build --root docs",
    "dev": "parcel src/index.html --global CookieThough",
    "dev:docs": "astro dev --root docs",
    "types": "tsc",
    "typeCheck": "tsc --noEmit && tsc --project tsconfig.test.json --noEmit",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "markdown:fix": "markdownlint '**/*.md' --ignore node_modules --fix",
    "prettier:fix": "npm run prettier -- --write",
    "prettier": "prettier \"src/**/*.{js,ts,tsx}\"",
    "prepare": "./scripts/postinstall-local.sh",
    "deploy:site": "firebase deploy --only hosting:site",
    "deploy:docs": "firebase deploy --only hosting:docs",
    "test": "jest",
    "test:coverage": "jest --coverage --reporters=jest-junit"
  },
  "eslintIgnore": [
    "dist/*"
  ],
  "dependencies": {
    "events": "3.3.0",
    "lodash.throttle": "4.1.1",
    "preact": "10.27.0"
  },
  "browserslist": [
    "> 0.25%",
    "last 2 versions",
    "IE >= 9"
  ],
  "devDependencies": {
    "@astrojs/mdx": "^4.3.12",
    "@astrojs/sitemap": "^3.6.0",
    "@babel/core": "7.28.5",
    "@babel/plugin-transform-react-jsx": "7.17.3",
    "@types/enzyme": "3.10.12",
    "@types/jest": "27.4.1",
    "@types/lodash.throttle": "4.1.7",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "browser-sync": "2.27.9",
    "cheerio": "1.1.2",
    "chokidar": "3.5.3",
    "chokidar-cli": "3.0.0",
    "dart-sass": "1.25.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-preact-pure": "4.1.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^4.2.5",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "firebase-tools": "14.12.0",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "jest-preset-preact": "4.1.1",
    "markdownlint-cli": "0.45.0",
    "npe": "1.2.0",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.5",
    "preact-render-to-string": "5.1.21",
    "prettier": "2.6.2",
    "sass": "^1.94.2",
    "typescript": "5.9.3"
  }
}
