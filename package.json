{
  "name": "cookie-though",
  "version": "0.0.0",
  "license": "MIT",
  "homepage": "https://cookie-though-docs.web.app",
  "scripts": {
    "docs": "docsify serve",
    "build:site": "tsc --noEmit && parcel build src/index.html --no-source-maps --global CookieThough --out-dir public --no-content-hash",
    "build:lib": "parcel build src/lib.ts --no-source-maps --global CookieThough --out-dir dist --experimental-scope-hoisting",
    "dev": "parcel src/index.html --global CookieThough",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "markdown:fix": "markdownlint '**/*.md' --ignore node_modules --fix",
    "prettier:fix": "npm run prettier -- --write",
    "prettier": "prettier \"src/**/*.{js,ts,tsx}\"",
    "prepare": "./scripts/postinstall-local.sh",
    "deploy:site": "firebase deploy --token $FIREBASE_TOKEN --only hosting:site",
    "deploy:docs": "./scripts/docsify.sh && firebase deploy --token $FIREBASE_TOKEN --only hosting:docs",
    "test": "jest",
    "test:coverage": "jest --coverage --reporters=jest-junit"
  },
  "eslintIgnore": [
    "dist/*"
  ],
  "dependencies": {
    "events": "3.2.0",
    "preact": "10.5.9"
  },
  "browserslist": [
    "> 0.25%",
    "last 2 versions",
    "IE >= 9"
  ],
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-transform-react-jsx": "7.12.12",
    "@types/enzyme": "3.10.8",
    "@types/jest": "26.0.20",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "css-loader": "5.0.1",
    "docsify-cli": "4.4.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-preact-pure": "3.0.0",
    "enzyme-to-json": "3.6.1",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "firebase-tools": "9.2.2",
    "jest": "26.6.3",
    "jest-junit": "12.0.0",
    "jest-preset-preact": "4.0.2",
    "markdownlint-cli": "0.26.0",
    "npe": "1.1.3",
    "parcel-bundler": "1.12.4",
    "prettier": "2.2.1",
    "sirv-cli": "1.0.10",
    "typescript": "4.1.3"
  }
}
