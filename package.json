{
  "name": "cookie-though",
  "version": "1.6.0",
  "license": "MIT",
  "homepage": "https://cookiethough.dev",
  "main": "dist/lib.js",
  "types": "dist/types/lib.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/inthepocket/cookie-though"
  },
  "keywords": [
    "cookies",
    "cookie",
    "cookie preferences",
    "browser",
    "universal"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build:site": "tsc --noEmit && parcel build src/index.html --no-source-maps --dist-dir public --no-content-hash && ./scripts/replace_css.sh staging",
    "build:lib": "parcel build src/lib.ts --no-source-maps --dist-dir dist && ./scripts/replace_css.sh",
    "build:docs": "npm run build:sass && npm run build:jekyll",
    "dev": "parcel src/index.html",
    "dev:site": "./scripts/site_exist.sh && npm-run-all -p serve watch:sass watch:jekyll",
    "types": "tsc",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "markdown:fix": "markdownlint '**/*.md' --ignore node_modules --fix",
    "prettier:fix": "npm run prettier -- --write",
    "prettier": "prettier \"src/**/*.{js,ts,tsx}\"",
    "prepare": "./scripts/postinstall-local.sh",
    "deploy:site": "firebase deploy --token $FIREBASE_TOKEN --only hosting:site",
    "deploy:docs": "firebase deploy --token $FIREBASE_TOKEN --only hosting:docs",
    "test": "jest",
    "test:coverage": "jest --coverage --reporters=jest-junit",
    "build:sass": "dart-sass --style=compressed docs/_scss/docs.scss:docs/jekyll/css/docs.min.css",
    "watch:sass": "chokidar 'docs/_scss/**/*' -c 'npm run build:sass'",
    "build:jekyll": "bundle exec jekyll build --trace",
    "watch:jekyll": "chokidar 'docs/jekyll/**' '_config.yml' -c 'npm run build:jekyll && browser-sync reload'",
    "serve": "browser-sync start --http --server _site"
  },
  "eslintIgnore": [
    "dist/*"
  ],
  "dependencies": {
    "events": "3.3.0",
    "lodash.throttle": "4.1.1",
    "preact": "10.27.0"
  },
  "browserslist": [
    "> 0.25%",
    "last 2 versions",
    "IE >= 9"
  ],
  "devDependencies": {
    "@babel/core": "7.28.0",
    "@babel/plugin-transform-react-jsx": "7.27.1",
    "@parcel/config-default": "^2.15.4",
    "@parcel/packager-ts": "^2.15.4",
    "@parcel/transformer-typescript-tsc": "^2.15.4",
    "@parcel/transformer-typescript-types": "^2.15.4",
    "@types/enzyme": "3.10.19",
    "@types/jest": "30.0.0",
    "@types/lodash.throttle": "4.1.9",
    "@types/node": "^18.19.122",
    "@typescript-eslint/eslint-plugin": "8.39.0",
    "@typescript-eslint/parser": "8.39.0",
    "browser-sync": "3.0.4",
    "chokidar": "4.0.3",
    "chokidar-cli": "3.0.0",
    "dart-sass": "1.25.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-preact-pure": "4.1.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "firebase-tools": "14.17.0",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "jest-preset-preact": "4.1.1",
    "markdownlint-cli": "0.45.0",
    "npe": "1.2.0",
    "npm-run-all": "4.1.5",
    "parcel": "^2.15.4",
    "preact-render-to-string": "6.5.13",
    "prettier": "3.6.2",
    "typescript": "5.9.2"
  }
}
